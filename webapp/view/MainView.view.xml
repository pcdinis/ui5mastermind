<mvc:View
	controllerName="pt.deploy.game.ui5mastermind.controller.MainView"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:u="sap.ui.unified"
>
	<u:Shell id="shell">
		<u:headEndItems>
			<u:ShellHeadItem icon="sap-icon://nav-back" tooltip="Back" press="handleBackPress"></u:ShellHeadItem>
		</u:headEndItems>
		<u:content>
			<App id="app">
				<pages>
					<Page id="page" title="{i18n>title}" titleAlignment="Center">
						<content>
							<FlexBox
								alignItems="Center"
								justifyContent="Center">
								<VBox id="idScrMainVBox">
									<!--<Button text="" icon="/images/yellowSquare.png" class="sapUiTinyMarginBegin"/>-->
								</VBox>
							</FlexBox>
						</content>
					</Page>
				</pages>
			</App>
		</u:content>
	</u:Shell>
</mvc:View>
